<root main_tree_to_execute="MainTree">
  <BehaviorTree ID="MainTree">
    <Repeat num_cycles="-1">
      <Sequence>
        <Fallback>
          <Sequence>
            <Condition ID="WaitForNewPiFrameProcessed" />
            <Fallback>
              <Sequence>
                  <Condition ID="ElevatorAtHeight" targetHeightPixels="342.0" z_tolerance_pixels="10.0" />
                  <Action ID="SaySomething" message="SUCCESS Elevator at height" />
              </Sequence>
              <Action ID="SaySomething" message="Elevator NOT at height" />
            </Fallback>
          </Sequence>
          <AlwaysSuccess />
        </Fallback>
        <Action ID="SleepSeconds" seconds="1.0" />
      </Sequence>
    </Repeat>
  </BehaviorTree>
</root>